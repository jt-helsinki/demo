{
  "name": "my-expo-app",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "dev": "expo start -c",
    "test": "vitest",
    "android": "expo start -c --android",
    "ios": "expo start -c --ios",
    "ios:run": "expo run --device \"iPhone 17 Pro\"",
    "ios:devices:list": "xcrun simctl list devices",
    "ios:runtimes:list": "xcrun simctl list runtimes",
    "ios:simulator": "open -a Simulator",
    "build:ios": "EXPO_DEBUG=1 eas build --platform ios",
    "build:ios:local": "EXPO_DEBUG=1 eas build --platform ios --local",
    "build:android": "eas build --platform android",
    "build:android:local": "eas build --platform android --local",
    "prebuild": "expo prebuild",
    "expo:doctor": "npx -y expo-doctor",
    "clean": "rm -rf .expo node_modules package-lock.json",
    "lint": "eslint src --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write --ignore-unknown \"*src/**/*.*\""
  },
